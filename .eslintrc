{
  "ecmaFeatures": {
    "jsx": true,
    "blockBindings": true,
    "defaultParams": true,
    "generators": true,
    "templateStrings": true,
    "forOf": true
  },
  "env": {
    "mocha": true,
    "node": true
  },
  "rules": {
    "quotes": [1, "single"],
    "no-multi-spaces": 1,
    "key-spacing": [1, {"align": "value"}]
  }
}
